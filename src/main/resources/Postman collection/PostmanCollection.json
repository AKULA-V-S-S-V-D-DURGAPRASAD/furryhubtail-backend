    {
      "name": "Auth - Login Customer",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"cust@example.com\",\n  \"password\": \"password123\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/auth/login",
          "host": ["{{baseUrl}}"],
          "path": ["api","auth","login"]
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test(\"Customer login returned token\", function () {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.have.property('token');",
              "});",
              "if (pm.response.code === 200) {",
              "    var jsonData = pm.response.json();",
              "    var rawToken = jsonData.token;",
              "    pm.environment.set('token', rawToken);",
              "}"
            ]
          }
        }
      ],
      "response": []
    },
